            @Html.DisplayFor(modelItem => item.cat_codigo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.car_descripcion)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new {  id=item.cat_codigo  }) |
